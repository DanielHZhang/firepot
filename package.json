    "firebase": "^7.8.2",
    "monaco-editor": "^0.20.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "express": "^4.17.1",
    "file-loader": "^5.1.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "tsc-silent": "^1.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yargs": "^15.1.0"
