  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --config test/webpack.config.js",
    "build": "rm -rf lib && tsc-silent -p tsconfig.json --suppress @",
